class pig():
	print("哼")
	count = 578
	def __init__(self,name):
		self.name = name
		self.__age = 578

	def getage(self):
		return self.__age




	@classmethod
	def getCount(cls):
		print("我是类方法")
		return cls.count



	@classmethod
	def getCount(cls):
		return cls.__count1




hh = pig("hH")
print(hh.name)
print(hh.getAge())
pig.getCount()
print(pig.getCount1())







